<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.studyboot.sms.dao.EndRateDao">

  <resultMap type="endRate" id="endRateMap">
    <result column="cmplt_pct" property="completePercent"/>
    <result column="drop_pct" property="dropPercent"/>
    <result column="exile_pct" property="exilePercent"/>
  </resultMap>

  <select id="findById" resultMap="endRateMap" parameterType="int">
    select
      cmplt_pct,
      drop_pct,
      exile_pct
    from
      sms_member_endrate
    where
      member_id = #{value}
  </select>


</mapper>










